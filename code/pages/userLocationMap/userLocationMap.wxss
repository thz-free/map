/* pages/userLocationMap/userLocationMap.wxss */
@import "../../lib/icon/icon-map.wxss";
.locationInfo{
  display: flex;
  flex-flow: row nowrap;
  height: 40rpx;
  padding: 18px 10px;
  border-bottom: 1px solid rgba(0,0,0,0.05);
}
.group{
  margin-left: 150rpx;
  display: flex;
  flex-flow: row nowrap;
}
